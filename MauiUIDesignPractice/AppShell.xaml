<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiUIDesignPractice.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiUIDesignPractice"
    xmlns:pages="clr-namespace:MauiUIDesignPractice.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="MauiUIDesignPractice">

    <ShellContent
